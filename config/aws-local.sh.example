#!/bin/bash
# Local AWS configuration overrides (gitignored)
#
# Copy this file to config/aws-local.sh and customize for your environment
# cp config/aws-local.sh.example config/aws-local.sh

# Example: Use a different region
# export AWS_REGION=us-west-2

# Example: Use a specific AWS profile
# export AWS_PROFILE=work

# Example: Use your IP for SSH access (more secure)
# export DEFAULT_SSH_LOCATION=203.0.113.0/32

# Example: Larger instances for your dev environment
# export DEFAULT_INSTANCE_TYPE=t3.medium

# Example: Enable debug output
# export AWS_CONFIG_DEBUG=true
